{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "bun run build",
    "installCommand": "bun i",
    "framework": null,
    "functions": {
        "api/index.js": {
            "runtime": "nodejs20.x"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index"
        }
    ]
}
